    </Entities>
    <EntityCache Enable="true" Timeout="30" Protocol="pipe"/>
  </EntitySettings>
  
  <CmsConfig>
    <CmsSettings CacheEnable="true" CacheTimeout="120" CacheProtocol="tcp" ConnectionKey="cnn_web"/>
  </CmsConfig>
  
  <RemoteCache>
    <CacheApiSettings>
      <add key="RemoteBundleHostName" value="nistec_cache_bundle"/>
      <add key="RemoteCacheHostName" value="nistec_cache_bundle"/>
      <add key="RemoteSyncCacheHostName" value="nistec_cache_bundle"/>
      <add key="RemoteSessionHostName" value="nistec_cache_bundle"/>
      <add key="RemoteDataCacheHostName" value="nistec_cache_bundle"/>
      <add key="IsRemoteAsync" value="false"/>
      <add key="EnableRemoteException" value="false"/>
    </CacheApiSettings>
    <TcpClientSettings>
      <host HostName="nistec_cache" Address="localhost" Port="13000" IsDuplex="true" IsAsync="false" SendTimeout="5000" ReadTimeout="1000" ReceiveBufferSize="4096" SendBufferSize="4096"/>
      <host HostName="nistec_cache_bundle" Address="localhost" Port="13000" IsDuplex="true" IsAsync="false" SendTimeout="500000" ReadTimeout="100000" ReceiveBufferSize="4096" SendBufferSize="4096"/>
    </TcpClientSettings>
  </RemoteCache>

  <connectionStrings configSource="connections.config"/>
    
  <appSettings file="C:\Dev\Domains\Co\References\Config\settings.config">
    <add key="baseUrl" value="http://localhost"/>
    <add key="WebCacheEnable" value="true"/>
    <add key="WebCacheTimeout" value="120"/>
    <add key="WebCacheProtocol" value="tcp"/>
    <add key="webpages:Version" value="2.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="PreserveLoginUrl" value="true"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <add key="UploadFileTypes" value=".csv|.txt"/>
