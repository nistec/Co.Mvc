      <Entity Name="EmailProvider" MappingName="web_EmailProvider" ConnectionKey="netcell_system" Mode="Config" SourceType="Table" EntityKey="Provider" LangResources=""/>
      <Entity Name="UserAuth" MappingName="sp_User_Auth" ConnectionKey="netcell_system" Mode="Config" SourceType="Procedure" EntityKey="UserId" LangResources=""/>
      <Entity Name="CmsSite" MappingName="web_CmsSite" ConnectionKey="netcell_system" Mode="Config" SourceType="Table" EntityKey="Provider" LangResources=""/>
