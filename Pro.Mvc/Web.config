  <EntitySettings>
    <Entities>
      <Entity Name="Authorizer" MappingName="sp_Ad_UserAuthenticate" ConnectionKey="netcell_system" Mode="Config" SourceType="Procedure" EntityKey="UserId" LangResources=""/>
      <Entity Name="AuthorizerService" MappingName="sp_Ad_UserAuthenticateService" ConnectionKey="netcell_system" Mode="Config" SourceType="Procedure" EntityKey="UserId" LangResources=""/>
      <Entity Name="UserProfile" MappingName="Ad_UserProfile" ConnectionKey="netcell_system" Mode="Config" SourceType="Table" EntityKey="UserId" LangResources=""/>
