@model Pro.Data.Registry.RegistryPage
@{
    Layout = "~/Views/Shared/_ViewRegistry.cshtml";
}

@section head {
    @Scripts.Render("~/bundles/jqx")

    @Html.Raw(Model.HScript)
}

@section scripts {
    <script type="text/javascript">
        $(document).ready(function () {
            var def = new registry_confirm();
        });
    </script>
@Html.Raw(Model.HiddenScript)

}

@section cmsHeader {
    <div id="cmsHeader">
        @Html.Raw(Model.Header)
    </div>
}
<input type="hidden" id="model-account" value="@Model.AccountId" />

<!--begin of content-wrapper-->
<div id ="cmsBody">
    @Html.Raw(Model.Body)
</div>

<div id="cmsArgs">
    @Html.Raw(Model.Args)
</div>


<!--end of content-wrapper-->

@section cmsFooter {
    <div id="cmsFooter">
        @Html.Raw(Model.Footer)
    </div>
}
